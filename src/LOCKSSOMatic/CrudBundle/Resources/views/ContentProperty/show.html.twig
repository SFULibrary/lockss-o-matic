{% extends '::base.html.twig' %}


{% block pageheader %}
    <h1>ContentProperty</h1>
{% endblock %}

{% block body -%}
	<ul class="record_actions">
		<li>
			<a href="{{ path('contentproperty') }}">
				Back to the list
			</a>
		</li>

		<li>
			<a href="{{ path('contentproperty_edit', { 'id': entity.id }) }}">
				<span class='glyphicon glyphicon-edit'></span>
			</a>
		</li>

		<li>
			<a href="{{ path('contentproperty_delete', { 'id': entity.id }) }}" class="js-confirm" data-confirm="Are you sure you want to delete this ContentProperty?">
				<span class='glyphicon glyphicon-remove'></span>
			</a>
		</li>
	</ul>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Propertykey</th>
                <td>{{ entity.propertyKey }}</td>
            </tr>
            <tr>
                <th>Propertyvalue</th>
                <td>{{ entity.propertyValue }}</td>
            </tr>
            <tr>
                <th>Islist</th>
                <td>{{ entity.isList }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
