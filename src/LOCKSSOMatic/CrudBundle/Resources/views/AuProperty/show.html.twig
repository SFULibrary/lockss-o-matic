{% extends '::base.html.twig' %}


{% block pageheader %}
    <h1>AuProperty</h1>
{% endblock %}

{% block body -%}
	<div class="btn-toolbar">
		<div class="btn-group pull-right">
			<a href="{{ path('auproperty') }}" class="btn btn-default">
				<span class="glyphicon glyphicon-arrow-left"></span> Back
			</a>
			<a href="{{ path('auproperty_edit', { 'id': entity.id }) }}" class="btn btn-default">
				<span class='glyphicon glyphicon-edit'></span> Edit
			</a>
			<a href="{{ path('auproperty_delete', { 'id': entity.id }) }}" class="btn btn-default js-confirm" data-confirm="Are you sure you want to delete this AuProperty?">
				<span class='glyphicon glyphicon-remove'></span> Delete
			</a>
		</div>
	</div>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Propertykey</th>
                <td>{{ entity.propertyKey }}</td>
            </tr>
            <tr>
                <th>Propertyvalue</th>
                <td>{{ entity.propertyValue }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
