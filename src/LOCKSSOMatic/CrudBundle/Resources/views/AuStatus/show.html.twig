{% extends '::base.html.twig' %}

    
{% block pageheader %}
    <h1>AuStatus</h1>
{% endblock %}
    
{% block body -%}
        <ul class="record_actions">
    <li>
        <a href="{{ path('austatus') }}">
            Back to the list
        </a>
    </li>

    <li>
        <a href="{{ path('austatus_edit', { 'id': entity.id }) }}">
            <span class='glyphicon glyphicon-edit'></span>
        </a>
    </li>

    <li>
        <a href="{{ path('austatus_delete', { 'id': entity.id }) }}" class="js-confirm" data-confirm="Are you sure you want to delete this AuStatus?">
            <span class='glyphicon glyphicon-remove'></span>
        </a>
    </li>
</ul>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Boxhostname</th>
                <td>{{ entity.boxHostname }}</td>
            </tr>
            <tr>
                <th>Querydate</th>
                <td>{{ entity.queryDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Propertykey</th>
                <td>{{ entity.propertyKey }}</td>
            </tr>
            <tr>
                <th>Propertyvalue</th>
                <td>{{ entity.propertyValue }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
