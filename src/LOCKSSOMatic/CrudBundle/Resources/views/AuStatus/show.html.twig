{% extends '::base.html.twig' %}


{% block pageheader %}
    <h1>AuStatus</h1>
{% endblock %}

{% block body -%}
	<div class="btn-toolbar">
		<div class="btn-group pull-right">
			<a href="{{ path('austatus') }}" class="btn btn-default">
				<span class="glyphicon glyphicon-arrow-left"></span> Back
			</a>
			<a href="{{ path('austatus_edit', { 'id': entity.id }) }}" class="btn btn-default">
				<span class='glyphicon glyphicon-edit'></span> Edit
			</a>
			<a href="{{ path('austatus_delete', { 'id': entity.id }) }}" class="btn btn-default js-confirm" data-confirm="Are you sure you want to delete this AuStatus?">
				<span class='glyphicon glyphicon-remove'></span> Delete
			</a>
		</div>
	</div>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Boxhostname</th>
                <td>{{ entity.boxHostname }}</td>
            </tr>
            <tr>
                <th>Querydate</th>
                <td>{{ entity.queryDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Propertykey</th>
                <td>{{ entity.propertyKey }}</td>
            </tr>
            <tr>
                <th>Propertyvalue</th>
                <td>{{ entity.propertyValue }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
