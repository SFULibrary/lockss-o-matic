<lockss-config>
{% for prop in pln.plnProperties %}
	{%- if prop.isList() -%}
	<property name="{{ prop.propertyKey }}">
		<list>
		{% for value in prop.getPropertyValue() -%}
			<value>{{ value }}</value>
		{% endfor -%}
		</list>
	</property>
	{%- else -%}
	<property name="{{ prop.propertyKey }}" value="{{ prop.getPropertyValue() }}"/>
	{% endif -%}
{% endfor -%}
</lockss-config>
